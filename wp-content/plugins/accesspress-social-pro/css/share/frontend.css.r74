    width: 35px;
    height: auto;
    text-align: center;
}
.apss-sticky-header-wrapper .apss-sticky-social-shares-wrapper .apss-icon-block {
    border-radius: 4px;
    padding: 2px 5px;
    color: #f1f1f1;
}
