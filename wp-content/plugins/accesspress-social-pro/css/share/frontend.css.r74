    font-size: 14px;
    padding-right: 10px;
}
.apss-sticky-header-wrapper .apss-header-logo {
    padding: 5px 20px;
}
.apss-sticky-header-wrapper .apss-header-logo img {
    max-width: 100%;
    height: 25px;
}
.apss-sticky-header-wrapper .apss-sticky-total-share-count {
